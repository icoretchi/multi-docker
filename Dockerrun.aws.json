{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "icoretchi/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "api",
      "image": "icoretchi/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "icoretchi/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "icoretchi/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappigs": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
